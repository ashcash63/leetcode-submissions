class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        length = len(nums)
        major = length//2
        count = {}
        max_major = 0
        max_n = 0

        for n in nums:
            if n not in count:
                count[n] = 0
            count[n] +=1
            if count[n] >= major and count[n] > max_major:
                max_major = count[n]
                max_n = n
        return max_n
            
  #O(n) & O(n)
